{
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "editor.rulers": [80, 110],
  "files.insertFinalNewline": true,
  "files.exclude": {
    "**/.*": false,
    "dist/": false,
    "node_modules/": false,
    "typings/": false,
    "**/*.config.*": false,
    "**/tsconfig*.json": false,
    "**/*.log*": false,
    "*LICENSE*": false,
    "yarn.lock": false,
    "firebase.json": false
  },
  "npm.packageManager": "yarn",
  "npm.autoDetect": "off",
  "typescript.tsc.autoDetect": "off",
  "typescript.tsdk": "node_modules/typescript/lib"
}
