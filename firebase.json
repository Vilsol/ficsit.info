{
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "assets/**",
        "headers": [
          { "key": "Cache-Control", "value": "max-age=31536000" }
        ]
      },
      {
        "source": "/v114480/**!(.*)",
        "headers": [
          { "key": "Link", "value": "</data/versions.json>;rel=preload;as=fetch,</data/v114480/buildings.json>;rel=preload;as=fetch,</data/v114480/index.json>;rel=preload;as=fetch,</data/v114480/items.json>;rel=preload;as=fetch,</data/v114480/recipes.json>;rel=preload;as=fetch,</data/v114480/schematics.json>;rel=preload;as=fetch" }
        ]
      },
      {
        "source": "/**!(.*)",
        "headers": [
          { "key": "Link", "value": "</data/versions.json>;rel=preload;as=fetch,</data/v114480/buildings.json>;rel=preload;as=fetch,</data/v114480/index.json>;rel=preload;as=fetch,</data/v114480/items.json>;rel=preload;as=fetch,</data/v114480/recipes.json>;rel=preload;as=fetch,</data/v114480/schematics.json>;rel=preload;as=fetch" }
        ]
      }
    ]
  }
}
