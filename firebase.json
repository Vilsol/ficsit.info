{
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "assets/**",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
        ]
      },
      {
        "source": "**/*.+([0123456789abcdef]).*",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
        ]
      },
      {
        "source": "/v115821/**!(.*)",
        "headers": [
          { "key": "Link", "value": "</data/v115821/entities.json>;rel=preload;as=fetch,</data/v115821/recipes.json>;rel=preload;as=fetch,</data/v115821/schematics.json>;rel=preload;as=fetch" }
        ]
      },
      {
        "source": "/**!(.*)",
        "headers": [
          { "key": "Link", "value": "</data/v115821/entities.json>;rel=preload;as=fetch,</data/v115821/recipes.json>;rel=preload;as=fetch,</data/v115821/schematics.json>;rel=preload;as=fetch" }
        ]
      }
    ]
  }
}
